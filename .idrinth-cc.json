{
    "verbosity": 2,
    "blacklist": ["/vendor", "/node_modules"],
    "xml": {
        "validity": false
    },
    "ini": {
        "validity": false
    },
    "json": {
        "validity": false
    },
    "yaml": {
        "validity": true,
        "mapping": {
            "mods.yml": "cwd://schema.json"
        },
        "additional-extensions": ["yml"]
    }
}
